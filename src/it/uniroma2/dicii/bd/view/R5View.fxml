                        <Insets left="30.0" top="30.0" />
                    </BorderPane.margin>
                    <font>
                        <Font size="35.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                    </VBox.margin>
                </Label>
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" right="10.0" top="5.0" />
            </BorderPane.margin>
        </VBox>
    </top>
